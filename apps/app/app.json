{
	"expo": {
		"name": "간격",
		"slug": "interval-app",
		"description": "지금 당장 말고, 조금 있다가",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "interval",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"backgroundColor": "#000000",
		"primaryColor": "#4CAF50",
		"splash": {
			"image": "./assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#000000"
		},
		"ios": {
			"supportsTablet": false,
			"bundleIdentifier": "com.interval.habit",
			"buildNumber": "1",
			"infoPlist": {
				"CFBundleDisplayName": "간격",
				"NSUserTrackingUsageDescription": "앱 사용 경험 개선을 위해 사용됩니다."
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#000000"
			},
			"package": "com.interval.habit",
			"versionCode": 1,
			"permissions": [
				"android.permission.SCHEDULE_EXACT_ALARM",
				"android.permission.POST_NOTIFICATIONS"
			],
			"blockedPermissions": [
				"android.permission.READ_EXTERNAL_STORAGE",
				"android.permission.WRITE_EXTERNAL_STORAGE"
			]
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png",
			"name": "간격",
			"shortName": "간격",
			"description": "습관과의 간격을 벌려주는 심리 타이머",
			"themeColor": "#4CAF50",
			"backgroundColor": "#000000"
		},
		"plugins": [
			"expo-router",
			"expo-secure-store",
			[
				"expo-notifications",
				{
					"icon": "./assets/images/icon.png",
					"color": "#4CAF50",
					"sounds": []
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "5e308357-44af-4486-a61a-2505c288e18c"
			}
		}
	}
}
