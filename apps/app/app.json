{
	"expo": {
		"name": "간격",
		"slug": "interval-app",
		"description": "지금 한 개비 말고, 조금 있다가 한 개비. 담배와의 간격을 벌려주는 심리 타이머 앱",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "./assets/images/icon.png",
		"scheme": "interval",
		"userInterfaceStyle": "automatic",
		"newArchEnabled": true,
		"backgroundColor": "#ffffff",
		"primaryColor": "#4CAF50",
		"splash": {
			"image": "./assets/images/splash-icon.png",
			"resizeMode": "contain",
			"backgroundColor": "#ffffff"
		},
		"ios": {
			"supportsTablet": false,
			"bundleIdentifier": "com.intervalapp.smoking",
			"buildNumber": "1",
			"infoPlist": {
				"CFBundleDisplayName": "간격",
				"NSUserTrackingUsageDescription": "앱 사용 경험 개선을 위해 사용됩니다."
			}
		},
		"android": {
			"adaptiveIcon": {
				"foregroundImage": "./assets/images/adaptive-icon.png",
				"backgroundColor": "#ffffff"
			},
			"package": "com.intervalapp.smoking",
			"versionCode": 1,
			"permissions": [
				"android.permission.SCHEDULE_EXACT_ALARM",
				"android.permission.POST_NOTIFICATIONS"
			],
			"blockedPermissions": [
				"android.permission.READ_EXTERNAL_STORAGE",
				"android.permission.WRITE_EXTERNAL_STORAGE"
			]
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "./assets/images/favicon.png",
			"name": "간격",
			"shortName": "간격",
			"description": "담배와의 간격을 벌려주는 심리 타이머",
			"themeColor": "#4CAF50",
			"backgroundColor": "#ffffff"
		},
		"plugins": [
			"expo-router",
			"expo-secure-store",
			[
				"expo-notifications",
				{
					"icon": "./assets/images/icon.png",
					"color": "#4CAF50",
					"sounds": []
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "7d1b3e20-837f-4beb-89c5-a51b1ea8470b"
			}
		},
		"locales": {
			"ko": "./locales/ko.json"
		}
	}
}
